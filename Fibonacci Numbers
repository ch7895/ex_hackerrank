object Solution {
    
     def fibonacci(x:Int):Int = {
         x match {
          case 0 | 1 => x
          case _ => fibonacci(x-1) + fibonacci(x-2)
         }
     
     }

    def main(args: Array[String]) {
         /** This will handle the input and output**/
         println(fibonacci(readInt()-1))

    }
}
